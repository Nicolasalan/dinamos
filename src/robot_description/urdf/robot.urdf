<?xml version="1.0" ?>
<robot name="robot">
  <!-- link: base_footprint -->
  <link name="base_footprint" />

  <!-- joint: base_footprint -> base_link -->
  <joint name="base_link_joint" type="fixed">
    <origin xyz="0 0 0" rpy="0 0 0" />
    <parent link="base_footprint" />
    <child link="base_link" />
  </joint>

  <!-- link: base_link -->
  <link name="base_link">
    <inertial>
      <origin xyz="0 0 0" rpy="0 0 0" />
      <mass value="1.0" />
      <inertia
                ixx="0.00646"
                ixy="0"
                ixz="0"
                iyy="0.00646"
                iyz="0"
                izz="0.01125"
            />
    </inertial>
    <visual>
      <material name="Red">
        <color rgba="1 0 0 1" />
      </material>
      <geometry>
        <cylinder length="0.1" radius="0.15" />
      </geometry>
    </visual>
    <collision>
      <geometry>
        <box size="0.25 0.25 0.10" />
      </geometry>
    </collision>
  </link>

  <!-- link: coluna -->
  <link name="coluna">
    <inertial>
      <mass value="0.5" />
      <inertia
                ixx="0.020529"
                ixy="0"
                ixz="0"
                iyy="0.020529"
                iyz="0"
                izz="0.000225"
            />
    </inertial>
    <visual>
      <!-- Desloca a geometria pra que a base da coluna fique no joint -->
      <origin xyz="0 0 0.35" rpy="0 0 0" />
      <material name="Red">
        <color rgba="1 0 0 1" />
      </material>
      <geometry>
        <cylinder length="0.7" radius="0.03" />
      </geometry>
    </visual>
    <collision>
      <geometry>
        <cylinder length="0.7" radius="0.03" />
      </geometry>
    </collision>
  </link>

  <!-- joint: base_link -> coluna -->
  <joint name="base_link_to_coluna" type="fixed">
    <!-- Posiciona o joint no topo da base (z = 0.05) -->
    <origin xyz="0 0 0.05" rpy="0 0 0" />
    <parent link="base_link" />
    <child link="coluna" />
  </joint>

  <!-- link: torso-->
  <link name="torso">
    <inertial>
      <mass value="0.8" />
      <inertia
                ixx="0.002167"
                ixy="0"
                ixz="0"
                iyy="0.002167"
                iyz="0"
                izz="0.003"
            />
    </inertial>
    <visual>
      <material name="Red">
    	<color rgba="1 0 0 1" />
  	  </material>
      <geometry>
        <box size="0.15 0.15 0.10" />
      </geometry>
    </visual>
    <collision>
      <geometry>
        <box size="0.15 0.15 0.10" />
      </geometry>
    </collision>
  </link>

  <!-- joint: coluna -> torso -->
  <joint name="coluna_to_torso" type="fixed">
    <origin xyz="0.0 0 0.7" rpy="0 0 0" />
    <parent link="coluna" />
    <child link="torso" />
  </joint>

  <link name="esfera_ombro_esquerdo">
    <inertial>
      <mass value="0.1" />
      <!-- I = 2/5*m*r^2 -->
      <inertia
                ixx="0.000016"
                ixy="0"
                ixz="0"
                iyy="0.000016"
                iyz="0"
                izz="0.000016"
            />
    </inertial>
    <visual>
      <material name="Blue">
    	<color rgba="0 0 1 1" />
  	  </material>
      <geometry>
        <sphere radius="0.02" />
      </geometry>
    </visual>
    <collision>
      <geometry>
        <sphere radius="0.02" />
      </geometry>
    </collision>
  </link>

  <!-- joint: torso -> esfera_ombro_esquerdo -->
  <joint name="torso_to_esfera_ombro" type="fixed">
    <origin xyz="0.0 0.085 0.0" rpy="0 0 0" />
    <parent link="torso" />
    <child link="esfera_ombro_esquerdo" />
  </joint>

  <!-- link: ombro_esquerdo-->
  <link name="ombro_esquerdo">
    <inertial>
      <mass value="0.2" />
      <inertia
                ixx="0.000672"
                ixy="0"
                ixz="0"
                iyy="0.000672"
                iyz="0"
                izz="0.000010"
            />
    </inertial>
    <visual>
      <material name="Blue">
    	<color rgba="0 0 1 1" />
  	  </material>
      <geometry>
        <cylinder length="0.20" radius="0.01" />
      </geometry>
      <origin xyz="0.0 0 0.10" rpy="0 0 0" />
    </visual>
    <collision>
      <geometry>
        <cylinder length="0.20" radius="0.01" />
      </geometry>
      <origin xyz="0.0 0 0.10" rpy="0 0 0" />
    </collision>
  </link>

  <!-- joint: esfera -> ombro_esquerdo -->
  <joint name="esfera_to_ombro_esquerdo" type="fixed">
    <origin xyz="0.0 0.0 0.0" rpy="3.14 0 0" />
    <parent link="esfera_ombro_esquerdo" />
    <child link="ombro_esquerdo" />
    <axis xyz="1 0 0" />
  </joint>

  <link name="esfera_ombro_direito">
    <inertial>
      <mass value="0.1" />
      <inertia
                ixx="0.000016"
                ixy="0"
                ixz="0"
                iyy="0.000016"
                iyz="0"
                izz="0.000016"
            />
    </inertial>
    <visual>
      <material name="Blue">
    	<color rgba="0 0 1 1" />
  	  </material>
      <geometry>
        <sphere radius="0.02" />
      </geometry>
    </visual>
    <collision>
      <geometry>
        <sphere radius="0.02" />
      </geometry>
    </collision>
  </link>

  <!-- joint: torso -> esfera_ombro_direito -->
  <joint name="torso_to_esfera_ombro_direito" type="fixed">
    <origin xyz="0.0 -0.085 0.0" rpy="0 0 0" />
    <parent link="torso" />
    <child link="esfera_ombro_direito" />
  </joint>

  <!-- link: ombro_direito-->
  <link name="ombro_direito">
    <inertial>
      <mass value="0.2" />
      <inertia
                ixx="0.000672"
                ixy="0"
                ixz="0"
                iyy="0.000672"
                iyz="0"
                izz="0.000010"
            />
    </inertial>
    <visual>
      <material name="Blue">
    	<color rgba="0 0 1 1" />
  	  </material>
      <geometry>
        <cylinder length="0.20" radius="0.01" />
      </geometry>
      <origin xyz="0.0 0 0.10" rpy="0 0 0" />
    </visual>
    <collision>
      <geometry>
        <cylinder length="0.20" radius="0.01" />
      </geometry>
      <origin xyz="0.0 0 0.10" rpy="0 0 0" />
    </collision>
  </link>

  <!-- joint: esfera -> ombro_direito -->
  <joint name="esfera_para_ombro_direito" type="fixed">
    <origin xyz="0.0 0.0 0.0" rpy="-3.14 0 0" />
    <parent link="esfera_ombro_direito" />
    <child link="ombro_direito" />
    <axis xyz="-1 0 0" />
  </joint>


</robot>
